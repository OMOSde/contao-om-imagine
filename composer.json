{
    "name": "omosde/contao-om-imagine",
    "type": "contao-bundle",
    "description": "Contao 4.4+ bundle to manipulate images during upload",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "RenÃ¨ Fehrmann",
            "homepage": "http://www.omos.de"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "contao/core-bundle": "^4.4",
        "wa72/htmlpagedom": "^1.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "OMOSde\\ContaoOmImagineBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "OMOSde\\ContaoOmImagineBundle\\ContaoManager\\Plugin"
    }
}
